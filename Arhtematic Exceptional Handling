import java.util.*;

 class Main{
    public static void main(String[] args){
        System.out.println("Main Program Started");
        int a=10,b=0,c;
        try{
            c=a/b;
            System.out.println(c);
        }catch(Exception e){
            System.out.println("Divide by zero Exception");
        }
        System.out.println("Main Program Ended");
    }
}

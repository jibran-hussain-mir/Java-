import java.util.*;

class Count{
    static int count;
    Count(){
        count++;
    }
}
public class Main{
    public static void main(String[] args){
    Count a=new Count();
    Count b=new Count();
    Count c=new Count();
    Count d=new Count();
    Count e=new Count();
    System.out.println("No. of Objects Created in this class are : " +Count.count);
}
}
